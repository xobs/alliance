TOP ( # IOs are ordered from left to right
   (IOPIN x(3).0 );
   (IOPIN x(2).0 );
   (IOPIN x(1).0 );
   (IOPIN x(0).0 );
   (IOPIN y(3).0 );
   (IOPIN y(2).0 );
   (IOPIN y(1).0 );
   (IOPIN y(0).0 );
)
BOTTOM ( # IOs are ordered from left to right
   (IOPIN r(7).0 );
   (IOPIN r(6).0 );
   (IOPIN r(5).0 );
   (IOPIN r(4).0 );
   (IOPIN r(3).0 );
   (IOPIN r(2).0 );
   (IOPIN r(1).0 );
   (IOPIN r(0).0 );
)
IGNORE ( # IOs are ignored(not placed) by IO Placer
)
