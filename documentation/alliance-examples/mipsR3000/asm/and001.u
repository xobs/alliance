	;  ###--------------------------------------------------------###
	;  #   and immediate						#
	;  ###--------------------------------------------------------###

data1		.equ	0xd6f2
data2		.equ	0xc451

		.org	0x00400000
		.start	init	

init:
		addiu	r6 , r0 , data1			; r6 = data2 
		andi	r7 , r6 , data2			; r7 = data1 & data2

		loadi	r3 , (data1 & data2)    	; good if r7 correct
		bne	r3 , r7 , bad        

		j	good
		nop		

		.org	0x004000d0
good:		j 	good
		nop		
bad:		j	bad
		nop		
		.end

