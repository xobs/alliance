;  # date	: nov 1996						#
;  # descr.	: functional test for mips				#
;  ###----------------------------------------------------------------###

	;  ###--------------------------------------------------------###
	;  #   conditional branch					#
	;  ###--------------------------------------------------------###

val1		.equ	0xa7a1
val2		.equ	0x2339

		.org	0x00400000
		.start	init

init :
		addi	r7 ,r0 , val1
		addi	r8 ,r0 , val2
		addi	r9 ,r0 , (val1 - val2)

		sub	r10 , r7 , r8

		bne	r10 , r9 , bad
		nop
		j	good
		nop

		.org	0x004000d0
good:		j	good
		nop
bad:		j	bad
		nop
		.end

