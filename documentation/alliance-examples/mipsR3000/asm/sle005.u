	;  ###--------------------------------------------------------###
	;  #   set if less or equal immediate (signed)			#
	;  ###--------------------------------------------------------###

data		.equ	4907

		.org	0x00400000
		.start	init

init:
		addiu	r11, r0 , 4907 
		slti	r14, r11, data		; ri = rj => 0
		beq	r14, r0, good
		nop
		j	bad
		nop

		.org	0x004000d0
good:		j	good
		nop
bad:		j	bad
		nop
		.end

