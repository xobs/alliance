	;  ###--------------------------------------------------------###
	;  #   set if less than immediate				#
	;  ###--------------------------------------------------------###

data		.equ 	8	
cste		.equ    1
		.org	0x00400000
		.start	init

init:
	   	addiu    r1 , r0 , cste	
		addiu	r13, r0 ,   data   
		sltiu	r14, r13, ( data - 2)
		beq	r14, r0 ,good
		nop
		j	bad
		nop

		.org	0x004000d0
good:		j	good
		nop
bad:		j	bad
		nop
		.end

