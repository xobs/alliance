ENTITY vfeed3 IS
GENERIC (
  CONSTANT area          : NATURAL := 3000;
  CONSTANT transistors	 : NATURAL := 0
);
PORT (
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END vfeed3;

ARCHITECTURE behaviour_data_flow OF vfeed3 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on vfeed3"
  SEVERITY WARNING;
END;
