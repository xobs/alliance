ENTITY vfeed8 IS
GENERIC (
  CONSTANT area          : NATURAL := 8000;
  CONSTANT transistors	 : NATURAL := 0
);
PORT (
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END vfeed8;

ARCHITECTURE behaviour_data_flow OF vfeed8 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on vfeed8"
  SEVERITY WARNING;
END;
