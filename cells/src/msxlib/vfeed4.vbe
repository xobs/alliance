ENTITY vfeed4 IS
GENERIC (
  CONSTANT area          : NATURAL := 4000;
  CONSTANT transistors	 : NATURAL := 0
);
PORT (
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END vfeed4;

ARCHITECTURE behaviour_data_flow OF vfeed4 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on vfeed4"
  SEVERITY WARNING;
END;
