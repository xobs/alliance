ENTITY vfeed5 IS
GENERIC (
  CONSTANT area          : NATURAL := 5000;
  CONSTANT transistors	 : NATURAL := 0
);
PORT (
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END vfeed5;

ARCHITECTURE behaviour_data_flow OF vfeed5 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on vfeed5"
  SEVERITY WARNING;
END;
