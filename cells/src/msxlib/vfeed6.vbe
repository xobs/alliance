ENTITY vfeed6 IS
GENERIC (
  CONSTANT area          : NATURAL := 6000;
  CONSTANT transistors	 : NATURAL := 0
);
PORT (
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END vfeed6;

ARCHITECTURE behaviour_data_flow OF vfeed6 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on vfeed6"
  SEVERITY WARNING;
END;
