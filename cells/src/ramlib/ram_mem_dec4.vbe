ENTITY ram_mem_dec4 IS
PORT (
  i0   	   : in  BIT;
  i1   	   : in  BIT;
  i2   	   : in  BIT;
  i3   	   : in  BIT;
  ndeca    : out  BIT;
  ndecb    : out  BIT;
  vdd  	   : in  BIT;
  vss	   : in  BIT
);
END ram_mem_dec4;

ARCHITECTURE VBE OF ram_mem_dec4 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on ram_mem_dec4"
  SEVERITY WARNING;

END;
