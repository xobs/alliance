ENTITY rf_inmux_buf_2 IS
PORT (
  ck     : in  BIT;
  sel    : in  BIT;
  nck 	 : out BIT;
  sel0	 : out BIT;
  sel1	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END rf_inmux_buf_2;

ARCHITECTURE VBE OF rf_inmux_buf_2 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on rf_inmux_buf_2"
  SEVERITY WARNING;

  nck <= not ck;
  sel1 <= sel;
  sel0 <= not sel;

END;
