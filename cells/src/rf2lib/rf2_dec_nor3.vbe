ENTITY rf2_dec_nor3 IS
PORT (
  i0     : in  BIT;
  i1     : in  BIT;
  i2     : in  BIT;
  nq   	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END rf2_dec_nor3;

ARCHITECTURE VBE OF rf2_dec_nor3 IS

BEGIN
  ASSERT (vdd and not (vss))
  REPORT "power supply is missing on rf2_dec_nor3"
  SEVERITY WARNING;

  nq <= not(i0 or i1 or i2);

END;
